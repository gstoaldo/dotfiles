{
  "title": "Delimiters",
  "maintainers": [
    "stoll"
  ],
  "rules": [
    {
      "description": "option for delimiter layer",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "option",
                "shift"
              ],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}