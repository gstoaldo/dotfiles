{
  "title": "Symbols",
  "maintainers": ["stoll"],
  "rules": [
    {
      "description": "option for symbols layer",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["option", "shift"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": "shift"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["option"],
              "optional": "caps_lock"
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
